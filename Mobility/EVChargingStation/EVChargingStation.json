                        {
                            "name": "ByInvoice",
                            "displayName": "ByInvoice",
                            "enumValue": 2
                        },
                        {
                            "name": "Cash",
                            "displayName": "Cash",
                            "enumValue": 3
                        },
                        {
                            "name": "CheckInAdvance",
                            "displayName": "CheckInAdvance",
                            "enumValue": 4
                        },
                        {
                            "name": "COD",
                            "displayName": "COD",
                            "enumValue": 5
                        },
                        {
                            "name": "DirectDebit",
                            "displayName": "DirectDebit",
                            "enumValue": 6
                        },
                        {
                            "name": "GoogleCheckout",
                            "displayName": "GoogleCheckout",
                            "enumValue": 7
