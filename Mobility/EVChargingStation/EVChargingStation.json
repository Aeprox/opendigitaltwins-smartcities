                            "enumValue": 4
                        },
                        {
                            "name": "motorcycle",
                            "displayName": "motorcycle",
                            "enumValue": 5
                        },
                        {
                            "name": "motorscooter",
                            "displayName": "motorscooter",
                            "enumValue": 6
                        },
                        {
                            "name": "truck",
                            "displayName": "truck",
                            "enumValue": 7
                        }
                    ]
                }
        },
        
