                            "enumValue": 9
                        },
                        {
                            "name": "Tesla",
                            "displayName": "Tesla",
                            "enumValue": 10
                        },
                        {
                            "name": "Type2",
                            "displayName": "Type2",
                            "enumValue": 11
                        },
                        {
                            "name": "Type3",
                            "displayName": "Type3",
                            "enumValue": 12
                        },
                        {
                            "name": "WallEuro",
                            "displayName": "Wall_Euro",
                            "enumValue": 13
