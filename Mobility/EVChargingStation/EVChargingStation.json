            "name": "contactPoint",
            "description": "Charging station contact point. Model:'https://schema.org/contactPoint'.",
            "schema": "string",
            "writable": true	
        },
        {
            "@type" : ["Property","Amperage"],
            "name": "amperage",
            "description": "The total amperage offered by the charging station.. Model:'http://schema.org/Number'. Units:'Ampers (A)'",
            "schema": "double",
            "unit": "amper",
            "writable": true	
        },
        
        {
            "@type" : ["Property","Voltage"],
            "name": "voltage",
            "description": "The total voltage offered by the charging station. Model:'http://schema.org/Number'. Units:'Volts (V)'",
            "schema": "double",
            "unit": "volt",
            "writable": true	
        },

        {
            "@type": "Property",
                "name": "chargeType",
                "description": "Vehicle type(s) which can be charged. Model:'http://schema.org/Text'. Enum:'bicycle, bus, car, caravan, motorcycle, motorscooter, truck' ",
                "schema": 
                {
                    "@type": "Enum",
                    "valueSchema": "integer",
                    "enumValues": [
                        {
                            "name": "annualPayment",
                            "displayName": "annualPayment",
                            "enumValue": 1
                        },
                        {
                            "name": "flat",
                            "displayName": "flat",
                            "enumValue": 2
                        },
                        {
                            "name": "free",
