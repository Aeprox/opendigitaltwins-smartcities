                        {
                            "name": "CHAdeMO",
                            "displayName": "CHAdeMO",
                            "enumValue": 2
                        },
                        {
                            "name": "CCSSAE",
                            "displayName": "CCSSAE",
                            "enumValue": 3
                        },
                        {
                            "name": "DualCHAdeMO",
                            "displayName": "Dual_CHAdeMO",
                            "enumValue": 4
                        },
                        {
                            "name": "DualJ1772",
                            "displayName": "Dual_J-1772",
                            "enumValue": 5
                        },
                        {
                            "name": "DualMennekes",
                            "displayName": "Dual_Mennekes",
                            "enumValue": 6
                        },
                        {
                            "name": "J1772",
                            "displayName": "J-1772",
                            "enumValue": 7
                        },
                        {
                            "name": "Mennekes",
                            "displayName": "Mennekes",
                            "enumValue": 8
                        },
                        {
